{
  "private": true,
  "workspaces": {
    "nohoist": [
      "**/react",
      "**/react-dom",
      "**/react-router-dom"
    ],
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --no-cache --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "devDependencies": {
    "eslint": "^8.4.1",
    "prettier": "^2.5.1",
    "tsconfig": "*",
    "turbo": "latest"
  },
  "packageManager": "npm@10.1.0"
}
